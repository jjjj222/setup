[user]
    name = jjjj222
    email = jjjj222.tso@gmail.com
[core]
    editor = vim
    excludesfile = ~/.jjjj222_setup/git/gitignore
    pager = less -R
[merge]
    tool = vimdiff
[color]
    ui = true
[difftool]
    prompt = false
[alias]
    #tree = log --graph --all --decorate --oneline --date-order
    tree = log --graph --all --pretty=format:'%C(yellow)%h%Creset%C(auto)%d%Creset %s %C(blue)(%cr) %C(magenta)<%an>%Creset' --abbrev-commit --date-order
    myalias = config --get-regexp alias
    aa = !git add -A && git status
    dp = diff HEAD^ HEAD
    ll = log --color --graph --stat --abbrev-commit
    t = !git tree
    cm = commit -m
    s = status
    d = diff
    showconfig = config --list --show-origin
[push]
    default = simple
