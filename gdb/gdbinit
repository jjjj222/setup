#=================== my setting ==================================
set print pretty on
set print object on
#set print array on
set pagination off
#set tui border-kind ascii
#set tui active-border-mode bold
set listsize 20
define ls
    if ( $argc == 0)
        list *$pc
    else
        set listsize $arg0
        list *$pc
        set listsize 20
    end
end

define ll
    ls 60
end

define goto
    tb $arg0
    jump $arg0
end

